{
  "name": "STargazer",
  "description": "A 'click-to-build' xpath and css selector generator",
  "version": "1.2.4",
  "manifest_version": 3,
  "icons": {
    "16": "assets/logo16.png",
    "32": "assets/logo32.png",
    "48": "assets/logo48.png",
    "128": "assets/logo128.png"
  },
  "permissions": [
    "storage",
    "scripting",
    "activeTab",
    "clipboardWrite"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "action": {
    "default_title": "Open Menu",
    "default_popup": "/pages/popup/index.html"
  },
  "commands": {
    "pause-execution": {
      "suggested_key": {
        "default": "Ctrl+I",
        "mac": "Command+I"
      },
      "description": "Pause execution of current page (only works with devtools open!)"
    },
    "start-picking": {
      "suggested_key": {
        "default": "Ctrl+Shift+P",
        "mac": "Command+Shift+P"
      },
      "description": "Start picking an element"
    }
  },
  "background": {
    "service_worker": "/src/background.js"
  },
  "devtools_page": "/pages/devtools/bootstrap.html",
  "web_accessible_resources": [
    {
      "resources": [
        "/pages/editor/index.html"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}